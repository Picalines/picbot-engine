{
    "name": "picbot-engine",
    "version": "1.1.1",
    "description": "discord.js bot engine",
    "type": "module",
    "module": "./dist/esm/index.js",
    "main": "./dist/commonjs/index.js",
    "exports": {
        "import": "./dist/esm/index.js",
        "default": "./dist/commonjs/index.js"
    },
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build:esm": "tsc -p tsconfig.json",
        "build:commonjs": "tsc -p tsconfig-cjs.json && printf '{\"type\":\"commonjs\"}' > dist/commonjs/package.json",
        "build": "npm run build:esm && npm run build:commonjs",
        "dev": "rm ./dist/ -r && npm link",
        "docs": "typedoc --options typedoc.json",
        "prepare": "npm run build"
    },
    "keywords": [
        "discord",
        "bot",
        "nodejs",
        "boilerplate",
        "engine"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Picalines/picbot-engine"
    },
    "author": "Picalines",
    "license": "ISC",
    "engines": {
        "node": ">=14.0.0"
    },
    "dependencies": {
        "discord.js": "^12.3.1"
    },
    "devDependencies": {
        "@types/node": "^13.13.12",
        "@types/ws": "^7.4.0",
        "typedoc": "^0.17.7",
        "typedoc-default-themes": "^0.10.1",
        "typedoc-thunder-theme": "0.0.2",
        "typescript": "^4.1.3"
    }
}
