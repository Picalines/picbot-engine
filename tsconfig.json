{
    "include": [
        "./src/",
    ],
    "exclude": [
        "./node_modules/",
        "**/*.spec.ts",
        "**/*.test.ts",
    ],
    "compilerOptions": {
        "strict": true,
        "noImplicitAny": true,
        "strictBindCallApply": true,
        "target": "ES2018",
        "module": "ES2020",
        "outDir": "./dist",
        "incremental": true,
        "removeComments": false,
        "declaration": true,
        "esModuleInterop": true,
        "moduleResolution": "classic",
        "baseUrl": "./src/",
        "paths": {
            "discord.js": ["../node_modules/discord.js/typings/index.d.ts"],
            "@discordjs/collection": ["../node_modules/@discordjs/collection/dist/index.d.ts"],
        },
    },
}