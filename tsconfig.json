{
    "include": [
        "./src/",
    ],
    "exclude": [
        "./node_modules/",
    ],
    "compilerOptions": {
        "outDir": "./dist",
        "declaration": true,
        "baseUrl": "./src/",
        "target": "ES2020",
        "module": "ES2020",
        "lib": ["ES2020"],
        "strict": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
    },
}